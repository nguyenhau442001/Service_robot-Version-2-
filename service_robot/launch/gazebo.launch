<?xml version="1.0" encoding="UTF-8"?>
<launch>

  <arg name="world" default="empty"/> 
  <arg name="paused" default="false"/>
  <arg name="use_sim_time" default="true"/>
  <arg name="gui" default="true"/>
  <arg name="headless" default="false"/>
  <arg name="debug" default="false"/>

  <arg name="x" default="3"/>
  <arg name="y" default="3"/>
  <arg name="z" default="0.5"/>
  <arg name="roll" default="0"/>
  <arg name="yaw" default="0"/>
  <arg name="pitch" default="0"/>
  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    
    <arg name="world_name" value="$(find service_robot)/worlds/nw.world"/>
    <arg name="paused" value="$(arg paused)"/>
    <arg name="use_sim_time" value="$(arg use_sim_time)"/>
    <arg name="gui" value="$(arg gui)"/>
    <arg name="headless" value="$(arg headless)"/>
    <arg name="debug" value="$(arg debug)"/>
  </include>

  
  <param name="robot_description" command="$(find xacro)/xacro '$(find service_robot)/urdf/mybot1.xacro'"/>
  <!-- <param name="robot_description" command="$(find xacro)/xacro '$(find barista_description)/urdf/barista/barista_hexagons_asus_xtion_pro.xacro'"/> -->
  <node name="mybot_spawn" pkg="gazebo_ros" type="spawn_model" output="screen"
   args="-urdf -param robot_description -model mybot
   -x $(arg x) -y $(arg y) -z $(arg z) -R $(arg roll) -P $(arg pitch) -Y $(arg yaw)" />

</launch>